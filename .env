# Expo will expose variables prefixed with EXPO_PUBLIC_* to both 
# native and web runtimes.

# Leaderboard API endpoint (also used by the server proxy route)
EXPO_PUBLIC_LAYER3_API_URL=https://layer3.xyz/api/assignment/users

# Ethplorer API configuration for on-chain data
EXPO_PUBLIC_ETHPLORER_BASE=https://api.ethplorer.io
EXPO_PUBLIC_ETHPLORER_KEY=freekey

# IPFS gateway configuration for avatar images
EXPO_PUBLIC_IPFS_GATEWAY=https://l3img-secondary.b-cdn.net/ipfs/
EXPO_PUBLIC_IPFS_IMAGE_QUERY=width=208&optimizer=image
